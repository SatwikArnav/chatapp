{
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/socket.io/$1"
    }
  ],
  "headers": [
    {
      "source": "/socket.io/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://chatapp-39y3.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
}
